{
  "name": "todo-frontend",
  "version": "0.1.0",
  "private": false,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
